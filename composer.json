{
    "name": "bertshang\/backup",
    "description": "backup tool",
    "license": "MIT",
    "authors": [
        {
            "name": "bertshang",
            "email": "359352960@qq.com"
        }
    ],

    "require": {
        "php": "^7.1",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/contracts": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/events": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/filesystem": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "illuminate/notifications": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "league/flysystem": "^1.0.27",
        "symfony/finder": "^3.3|^4.0"
    },

    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "orchestra/testbench" : "~3.5.0|~3.6.0|~3.7.0|~3.8.0",
        "mockery/mockery": "^1.0"
    },

    "autoload": {
        "psr-4": {
            "Bertshang\\Backup\\": "src"
        },

        "files": [
            "src/Helpers/functions.php"
        ]
    },

    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bertshang\\Backup\\BackupServiceProvider"
            ]
        }
    }
}